objectclass ( 1.3.6.1.4.1.666.1.1 NAME 'spbPerson'
		SUP ( inetOrgPerson $ pilotPerson ) STRUCTURAL
		MUST spb-gender MAY ( spb-dateOfBirth $ spb-subBalance $ spb-instrument $ spb-employerName $ spb-warrantHolder $ spb-joinDate) )

objectclass ( 1.3.6.1.4.1.666.1.2 NAME 'spbLocation'
		SUP top AUXILIARY
		MAY ( spb-houseIdentifierLocation $ spb-streetLocation $ spb-boroughLocation $ spb-townLocation $ spb-countyLocation $ spb-postcodeLocation $ spb-latLocation $ spb-longLocation $ spb-countryLocation ) ) 

objectclass ( 1.3.6.1.4.1.666.1.3 NAME 'spbEvent'
		SUP top
		MUST ( cn $ spb-displayName $ spb-date $ spb-time $ spb-subPrice $ spb-eventType )
		MAY ( spb-uniqueAttendee $ spb-uniqueIntendedAttendee $ spb-uniqueIntendedAbsentee $ spb-uniqueSubPayer $ spb-subCharged $ description $ spb-websiteAnchorName $ spb-subPayment $ spb-intendanceRequiredDate $ spb-lastIntendanceReminderDate $ spb-timeOnSite $ spb-bandRoleInEvent $ spb-requesterName $ spb-requesterEmail $ spb-requesterLandline $ spb-requesterMobile $ spb-requesterHouseIdentifierLocation $ spb-requesterStreetLocation $ spb-requesterBoroughLocation $ spb-requesterTownLocation $ spb-requesterCountyLocation $ spb-requesterPostcodeLocation $ spb-requesterCountryLocation $ spb-requesterWebsite $ spb-requesterOrgName $ spb-requesterFreeText $ spb-eventStatus ) )
